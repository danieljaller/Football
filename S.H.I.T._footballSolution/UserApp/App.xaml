<Application x:Class="UserApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UserApp"
             xmlns:converter="clr-namespace:UserApp.Converters"
             StartupUri="MVVM_Views/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converter:ObjectToStringConverter x:Key="ObjectToStringConverter"/>
            <local:ViewModelLocator x:Key="ViewModelLocator"></local:ViewModelLocator>
        </ResourceDictionary>
    </Application.Resources>
</Application>
