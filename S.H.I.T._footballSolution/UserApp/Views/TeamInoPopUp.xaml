<Window x:Class="UserApp.Views.TeamInoPopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserApp.Views"
        mc:Ignorable="d"
        Title="TeamInoPopUp" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Frame Name="mainFrame" Grid.Row="0" Margin="10"/>
        <Button Name="Ok" Grid.Row="1" Width="100" Click="Ok_Click">OK</Button>
    </Grid>
</Window>
